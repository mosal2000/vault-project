{
  "backend": {
    "file": {
      "path": "vault/data"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "tls_disable": 0 ,
      "tls_cert_file": "/etc/ssl/vault.pem",
      "tls_key_file": "/etc/ssl/ronosentono.com.key"
    }
  },
  "log_level": "warn",
  "ui": true
}
